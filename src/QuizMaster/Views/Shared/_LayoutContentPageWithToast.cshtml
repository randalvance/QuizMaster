@{
    Layout = "_LayoutContentPage";
}
@RenderBody()


@section head {
    <link href="~/lib/toastr/build/toastr.min.css" rel="stylesheet" />
}

@section scripts {
    <script src="~/lib/toastr/build/toastr.min.js"></script>
    <script src="~/scripts/ToastHelper.js"></script>
    <script type="text/javascript">
        ToastHelper.showNotification('@Html.Raw(ViewBag.ToastOptions)');
    </script>
    @RenderSection("scripts", false)
}